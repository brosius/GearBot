package org.usfirst.frc.team6317.robot.commands;

import org.usfirst.frc.team6317.robot.OI;
import org.usfirst.frc.team6317.robot.Robot;

import edu.wpi.first.wpilibj.command.Command;

public class DefaultLiftCommand extends Command {
	
	public DefaultLiftCommand() {
		requires(Robot.LiftSubsystem);
	}
	
	@Override
	protected void execute() {
		Robot.LiftSubsystem.set(OI.controlStick.getThrottle());
		Robot.LiftSubsystem.set(OI.controlStick.getY());
	}

	@Override
	protected boolean isFinished() {
		return false;
	}

}
